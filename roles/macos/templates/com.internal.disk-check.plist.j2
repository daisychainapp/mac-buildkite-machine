<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.internal.disk-check</string>

    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/check-disk-space.sh</string>
    </array>

    <key>StartInterval</key>
    <integer>3600</integer>

    <key>RunAtLoad</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/var/log/disk-check.log</string>

    <key>StandardErrorPath</key>
    <string>/var/log/disk-check.error.log</string>
</dict>
</plist>
