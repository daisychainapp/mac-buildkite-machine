# Build secrets - sourced by Buildkite environment hook
# Managed by Ansible - do not edit manually
# This file contains sensitive data - mode 0600

# Buildkite agent token (also in config, but available for scripts)
BUILDKITE_AGENT_TOKEN="{{ vault_buildkite_agent_token }}"

# Add additional secrets below as needed
# These are defined in group_vars/vault.yml

{% if vault_npm_token is defined and vault_npm_token %}
NPM_TOKEN="{{ vault_npm_token }}"
{% endif %}

{% if vault_docker_hub_token is defined and vault_docker_hub_token %}
DOCKER_HUB_TOKEN="{{ vault_docker_hub_token }}"
{% endif %}

{% if vault_aws_access_key_id is defined and vault_aws_access_key_id %}
AWS_ACCESS_KEY_ID="{{ vault_aws_access_key_id }}"
{% endif %}

{% if vault_aws_secret_access_key is defined and vault_aws_secret_access_key %}
AWS_SECRET_ACCESS_KEY="{{ vault_aws_secret_access_key }}"
{% endif %}
